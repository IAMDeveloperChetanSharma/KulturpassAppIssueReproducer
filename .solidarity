{
  "$schema": "http://json.schemastore.org/solidaritySchema",
  "requirements": {
    "Node": [
      {
        "rule": "cli",
        "binary": "node",
        "semver": "18.13.0"
      }
    ],
    "Watchman": [
      {
        "rule": "cli",
        "binary": "watchman",
        "error": "Please install watchman on this machine. Refer to the official Watchman installation instructions for additional help.",
      }
    ],
    "React Native": [
      {
        "rule": "cli",
        "binary": "react-native",
        "semver": "9.3.2"
      }
    ],
    "Package JSON": [
      {
        "rule": "file",
        "location": "./package.json"
      }
    ],
    "NPM": [
      {
        "rule": "cli",
        "binary": "npm",
        "semver": "9.2.0"
      }
    ],
    "TypeScript": [
      {
        "rule": "cli",
        "binary": "tsc",
        "semver": "4.9.4"
      }
    ],
    "Yarn": [
      {
        "rule": "cli",
        "binary": "yarn",
        "version": "--version",
        "semver": "1.22.19"
      }
    ],
    "CocoaPods": [
      {
        "rule": "cli",
        "binary": "pod",
        "semver": "1.11.3"
      },
      {
        "rule": "dir",
        "location": "./ios/Pods"
      }
    ],
    "Android": [
      {
        "rule": "env",
        "variable": "ANDROID_SDK_ROOT",
        "error": "The ANDROID_SDK_ROOT environment variable must be set to your local SDK.  Refer to getting started docs for help."
      }
    ],
    "Xcode": [
      {
        "rule": "cli",
        "binary": "xcodebuild",
        "semver": "14.2"
      },
      {
        "rule": "cli",
        "binary": "xcrun",
        "semver": "61"
      }
    ]
  }
}