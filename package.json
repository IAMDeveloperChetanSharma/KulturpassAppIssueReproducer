{
  "name": "cultureapp",
  "version": "0.7.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-reverse": "adb reverse tcp:8081 tcp:8081",
    "android-menu": "adb shell input keyevent 82",
    "android-reload": "adb shell input keyevent 82 && adb shell input keyevent 66 && adb shell input keyevent 66",
    "ci": "yarn tsc && yarn lint && yarn test-ci",
    "ios": "react-native run-ios",
    "ios:se": "react-native run-ios --simulator=\"iPhone SE (3rd generation)\"",
    "start": "react-native start",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-ci": "jest --runInBand --coverage",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint-staged": "lint-staged --config lint-staged.js",
    "lint-fix": "yarn lint --fix",
    "lint-report": "yarn lint --format json --output-file coverage/eslint-result.json",
    "clean": "watchman watch-del-all && rm -rf node_modules/ && yarn cache clean && yarn install",
    "clean-start": "yarn clean && yarn start",
    "clean-pods": "rm -rf ios/Pods && yarn podinstall",
    "clean-gradle": "pushd android && ./gradlew clean && rm -rf .gradle build && popd",
    "clean-all": "yarn clean-pods && yarn clean-gradle && yarn clean-start",
    "prettier": "prettier \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "prettier-check": "yarn prettier --check",
    "prettier-write": "yarn prettier -- --write",
    "tsc": "tsc --noEmit --skipLibCheck",
    "xlint": "yarn tsc && yarn lint && yarn prettier-check",
    "ci-all": "yarn xlint && yarn lint-report",
    "podinstall": "cd ios && rm -f Podfile.lock && USE_FLIPPER=true bundle exec pod install",
    "podinstall-release": "cd ios && rm -f Podfile.lock && bundle exec pod install",
    "reinstall": "rm -rf iod/Pods && rm -rf node_modules && yarn install && yarn podinstall",
    "prestart": "sb-rn-get-stories",
    "storybook-watcher": "sb-rn-watcher",
    "prepare": "is-ci || husky install",
    "types-commerce": "npx ts-node ./scripts/types-commerce.ts",
    "ausweisapp2-fix-postinstall": "rm -rf node_modules/@jolocom/react-native-ausweis/ios/Frameworks/AusweisApp2-SDK-iOS/ && cd node_modules/@jolocom/react-native-ausweis && yarn postinstall && cd ../../.. && yarn podinstall"
  },
  "dependencies": {
    "@craftzdog/react-native-buffer": "^6.0.5",
    "@hookform/resolvers": "^2.9.11",
    "@jolocom/react-native-ausweis": "https://github.com/milanpro/react-native-ausweis.git",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-clipboard/clipboard": "^1.11.2",
    "@react-navigation/bottom-tabs": "^6.5.3",
    "@react-navigation/material-top-tabs": "^6.6.2",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/stack": "^6.3.11",
    "@reduxjs/toolkit": "^1.9.1",
    "appcenter": "^4.4.5",
    "appcenter-analytics": "^4.4.5",
    "appcenter-crashes": "^4.4.5",
    "axios": "^1.3.3",
    "i18next": "^22.4.9",
    "i18next-react-native-async-storage": "^1.0.4",
    "intl-pluralrules": "^1.3.1",
    "js-yaml": "^4.1.0",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "react": "18.1.0",
    "react-hook-form": "^7.43.0",
    "react-i18next": "^12.1.4",
    "react-native": "0.70.7",
    "react-native-date-picker": "^4.2.6",
    "react-native-device-info": "^10.3.0",
    "react-native-dotenv": "^3.4.7",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-get-random-values": "^1.8.0",
    "react-native-pager-view": "^6.1.4",
    "react-native-permissions": "^3.6.1",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-quick-base64": "^2.0.5",
    "react-native-quick-crypto": "^0.5.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0",
    "react-native-svg": "^13.8.0",
    "react-native-system-navigation-bar": "^2.2.0",
    "react-native-tab-view": "^3.4.1",
    "react-native-webview": "^11.26.0",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.2",
    "stream-browserify": "^3.0.0",
    "swagger-typescript-api": "^12.0.3",
    "uuid": "^9.0.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/datetimepicker": "^6.7.3",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native-community/slider": "^4.4.2",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-ondevice-actions": "^6.0.1-beta.12",
    "@storybook/addon-ondevice-controls": "^6.0.1-beta.12",
    "@storybook/core-common": "^6.5.16",
    "@storybook/react-native": "^6.0.1-beta.12",
    "@testing-library/jest-native": "^5.4.1",
    "@testing-library/react-native": "^11.5.1",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.throttle": "^4.1.7",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-content-transformer": "^0.2.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.0",
    "is-ci": "^3.0.1",
    "jest": "^26.6.3",
    "jest-junit": "^15.0.0",
    "metro-react-native-babel-preset": "0.72.3",
    "msw": "^1.0.1",
    "react-dom": "^18.2.0",
    "react-native-flipper": "0.125.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.1.0",
    "redux-flipper": "^2.0.2",
    "solidarity": "^3.0.4",
    "solidarity-react-native": "^2.1.2",
    "typed-emitter": "^2.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/env.ts"
    ],
    "coverageReporters": [
      "cobertura",
      "text-summary",
      "lcov"
    ],
    "setupFiles": [
      "./jest-setup.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@?react-native|@react-native-community|@react-navigation)"
    ],
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgMock.js",
      "axios": "axios/dist/node/axios.cjs"
    }
  }
}
